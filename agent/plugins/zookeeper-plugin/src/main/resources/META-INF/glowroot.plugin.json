{
  "name": "ZooKeeper Plugin",
  "id": "zookeeper",
  "instrumentation": [
    {
      "className": "org.apache.zookeeper.ZooKeeper",
      "methodName": "create",
      "methodParameterTypes": [
        "*",
        ".."
      ],
      "nestingGroup": "zookeeper",
      "captureKind": "trace-entry",
      "timerName": "zookeeper create",
      "traceEntryMessageTemplate": "zookeeper create {{0}}"
    },
    {
      "className": "org.apache.zookeeper.ZooKeeper",
      "methodName": "delete",
      "methodParameterTypes": [
        "*",
        ".."
      ],
      "nestingGroup": "zookeeper",
      "captureKind": "trace-entry",
      "timerName": "zookeeper delete",
      "traceEntryMessageTemplate": "zookeeper delete {{0}}"
    },
    {
